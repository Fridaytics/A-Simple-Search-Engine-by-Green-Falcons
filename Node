
	public class Node<T> {
		public T key;
		public Node<T> next;

		public Node () {
			data = null;
			next = null;
		}

		public Node (T val) {
			data = val;
			next = null;
		}

		public T getData() {
			return key;
		}

		public void setData(T data) {
			this.data = data;
		}

		public Node<T> getNext() {
			return next;
		}

		public void setNext(Node<T> next) {
			this.next = next;
		}

	

}
